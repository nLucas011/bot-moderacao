{
  "name": "typescript",
  "version": "1.0.0",
  "description": "Discord Bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "build": "tsc",
    "dev": "tsx --env-file .env ./src/index.ts",
    "dev:dev": "tsx --env-file .env.development ./src/index.ts",
    "watch": "tsx --watch --env-file .env ./src/index.ts",
    "watch:dev": "tsx --watch --env-file .env.development ./src/index.ts",
    "start": "node --env-file .env .",
    "start:dev": "node --env-file .env.development ."
  },
  "dependencies": {
    "@iamtraction/google-translate": "github:iamtraction/google-translate",
    "@magicyan/discord": "^1.0.21",
    "better-sqlite3": "^9.4.3",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "discord.js": "^14.14.1",
    "fast-glob": "^3.3.2",
    "ms": "^2.1.3",
    "quick.db": "^9.1.7"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "pretty-ms": "^9.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "imports": {
    "#base": [
      "./dist/discord/base/index.js"
    ],
    "#settings": [
      "./dist/settings/index.js"
    ],
    "#database": [
      "./dist/database/index.js"
    ],
    "#functions": [
      "./dist/functions/index.js"
    ],
    "#classes": [
      "./dist/classes/index.js"
    ],
    "#tools": [
      "./dist/tools/index.js"
    ],
    "#menus": [
      "./dist/menus/index.js"
    ]
  }
}
